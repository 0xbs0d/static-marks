    "prettier": "1.11.1"
  },
  "files": [
    "bin",
    "src"
  ],
  "homepage": "https://staticmarks.com",
  "keywords": [
    "bookmarks",
    "filter",
    "share",
    "yaml"
  ],
  "license": "MIT",
  "main": "bin/static-marks",
  "repository": {
    "type": "git",
    "url": "https://github.com/darekkay/static-marks.git"
  },
  "scripts": {
    "format": "prettier --write -- \"{src,bin,test}/**/*.{js,json}\"",
